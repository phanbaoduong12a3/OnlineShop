@model IEnumerable<Model.EF.About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="main">
    <img src="~/assests/client/images/1234567.png" style="width:100%;margin-bottom:30px"/>
    @foreach (var item in Model)
    {
        <div style="width:100%">
            <div style="margin-bottom:50px">
                <img width="100%" style="height:300px;width:300px;float:left" src="@item.Image" alt=""/>
            
            
                <div style="width:100%;margin-left:30px" >
                    <p style="color:yellow;font-size:28px;margin:20px 0 20px 0">@item.Name</p>
                    <p style="font-size:20px;margin-bottom:20px">@item.Description</p>
                    <hr style="width:300px"/>
                    <p style="text-align: justify;font-size: 20px; margin: 20px 0 20px 0">@item.Detal</p>
                </div>
            </div>
        </div>
        
    }

</div>


